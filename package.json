{
  "name": "financial-dashboard-saas",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "workspaces": [
    "web",
    "api"
  ],
  "scripts": {
    "dev": "npm run dev --workspace=web",
    "dev:api": "npm run dev --workspace=api",
    "dev:full": "concurrently \"npm run dev:api\" \"npm run dev\"",
    "build": "npm run build --workspace=web && npm run build --workspace=api",
    "test": "npm run test --workspaces",
    "lint": "npm run lint --workspaces",
    "db:migrate": "npm run db:migrate --workspace=api",
    "db:seed": "npm run db:seed --workspace=api"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "concurrently": "^8.2.2",
    "eslint": "^9.34.0",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.3.0",
    "jiti": "^2.5.1",
    "typescript-eslint": "^8.42.0"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.85.9",
    "@types/papaparse": "^5.3.16",
    "@types/react-grid-layout": "^1.3.5",
    "glob": "^11.0.3",
    "multer": "^3.0.0-alpha.1",
    "papaparse": "^5.5.3",
    "react-grid-layout": "^1.5.2",
    "recharts": "^3.1.2"
  }
}
